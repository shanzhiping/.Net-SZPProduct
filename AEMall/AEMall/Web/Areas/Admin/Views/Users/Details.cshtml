            <td>
                @Model.IsPower
            </td>
		</tr>

        <tr>
            <th>
                @Html.LabelFor(model => model.IsActive)
            </th>
            <td>
                @Model.IsActive
            </td>
		</tr>

        <tr>
            <th>
                @Html.LabelFor(model => model.RegionId)
            </th>
            <td>
                @Model.RegionId
            </td>
		</tr>

        <tr>
            <th>
                @Html.LabelFor(model => model.Mobil)
