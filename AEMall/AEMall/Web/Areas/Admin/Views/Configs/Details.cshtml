@model Models.Configs
@{
    ViewBag.Title = "Details";
}
<div id="buttons">
    @{Html.RenderPartial("Button_Back2List");}
</div>
<fieldset>
    <legend>详细信息</legend>
    <table cellpadding="5px" cellspacing="1">
        <tr>
            <th>
                @Html.LabelFor(model => model.Name)
            </th>
            <td>
                @Model.Name
            </td>
		</tr>

        <tr>
            <th>
                @Html.LabelFor(model => model.IsValid)
            </th>
            <td>
                @Html.DisplayFor(model => model.IsValid)
            </td>
		</tr>

        <tr>
            <th>
                @Html.LabelFor(model => model.Content)
            </th>
            <td>
                @Model.Content
            </td>
		</tr>
    </table>
</fieldset>

